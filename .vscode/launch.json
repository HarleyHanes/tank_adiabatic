{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Control Analysis (module)",
            "type": "debugpy",
            "request": "launch",
            "module": "PodRomAnalysis.controlAnalysis",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": true
        },
        {
            "name": "Python: Sensitivity Analysis (module)",
            "type": "debugpy",
            "request": "launch",
            "module": "PodRomAnalysis.sensitivityAnalysis",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": true
        },
        {
            "name": "Python: Current File (workspace cwd)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": true
        }
    ]
}
